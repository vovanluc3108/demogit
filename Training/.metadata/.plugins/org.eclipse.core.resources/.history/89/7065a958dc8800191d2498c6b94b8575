package homework;

public class Category {
 public String name;
 private double startlv; 
 private double endlv;
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public double getStartlv() {
	return startlv;
}
public void setStartlv(double startlv) {
	this.startlv = startlv;
}
public double getEndlv() {
	return endlv;
}
public void setEndlv(double endlv) {
	this.endlv = endlv;
} 
 
public Category(String name, double startlv, double endlv) {
	this.name = name;
	this.startlv = startlv;
	this.endlv = endlv;
	}
 


}
